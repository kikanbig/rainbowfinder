# RainbowFinder Mobile App Environment Variables
# Скопируйте этот файл в .env.local и обновите значения

# OpenWeatherMap API Key
EXPO_PUBLIC_OPENWEATHER_API_KEY=be314fc63d2ce6bc8c976a4e60be8955

# Backend API URL (для разработки)
EXPO_PUBLIC_API_URL=http://localhost:3000/api

# Backend API URL (для production - обновите после деплоя на Railway)
# EXPO_PUBLIC_API_URL=https://your-railway-app.railway.app/api

# Дополнительные настройки
EXPO_PUBLIC_DEBUG_MODE=true
EXPO_PUBLIC_LOG_LEVEL=info 